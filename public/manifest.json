{
  "manifest_version": 2,
  "name": "即刻黄历",
  "description": "My first chrome extension.",
  "version": "1.0.0",
  "icons": {
    "16": "image/icon.png",
    "128": "image/icon.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "image/icon.png",
      "38": "image/icon.png"
    },
    "default_title": "即刻黄历",
    "default_popup": "index.html"
  },
  "permissions": [ "activeTab", "alarms", "background", "notifications", "storage", "tabs", "\u003Call_urls>" ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy":"script-src 'self' 'sha256-18j9+nErtuKtcSwKvHjJxqQJK1kCjNV0fFqZIbKBDDY=' https://apis.google.com; object-src 'self'"
}